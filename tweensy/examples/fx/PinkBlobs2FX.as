package {	import flash.display.*;	import flash.filters.BlurFilter;	import flash.filters.GlowFilter;	import flash.geom.ColorTransform;	import flash.utils.getDefinitionByName;		import com.flashdynamix.motion.*;	import com.flashdynamix.motion.effects.core.*;	import com.flashdynamix.motion.layers.BitmapLayer;	import com.flashdynamix.utils.SWFProfiler;		import fl.motion.easing.*;		/**	 * @author shanem	 */	public class PinkBlobs2FX extends Sprite {				private var tween : TweensyGroup;		private var layer : BitmapLayer;		private var bf : BlurFilter;		public function PinkBlobs2FX() {			SWFProfiler.init(this);						tween = new TweensyGroup(false, true);			bf = new BlurFilter(20, 20, 1);			layer = new BitmapLayer(550, 400, 1);						layer.add(new ColorEffect(new ColorTransform(1, 1, 1, 0.99)));			layer.add(new FilterEffect(bf));			layer.add(new ThresholdEffect("<=", 0x30FFFFFF));						layer.blendMode = BlendMode.ADD;						var gf : GlowFilter = new GlowFilter(0xFF0000, 1, 2, 2, 2, 2, false, false);			layer.filters = [gf];						stage.quality = StageQuality.LOW;			addChild(layer);						var Box : Class = getDefinitionByName("Box") as Class;			var holder : Sprite = new Sprite();			for(var i : int = 0;i < 60; i++) {								var item : Sprite = new Box();				item.y = -100;				item.x = Math.random() * 500;				item.scaleX = item.scaleY = 2 + (Math.random() * 3);												var ct : ColorTransform = new ColorTransform();				ct.color = 0xFF5EBF;				item.transform.colorTransform = ct;								holder.addChild(item);				tween.to(item, {y:"650", x:"-50, 50"}, 2 + Math.random() * 4, Linear.easeNone, Math.random() * 3).repeatType = TweensyTimeline.YOYO;			}						layer.draw(holder);		}	}}