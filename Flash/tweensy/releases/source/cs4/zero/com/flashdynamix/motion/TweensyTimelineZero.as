	/**	 * TweensyTimelineZero is used by the TweensyZero class as a simple version of the TweensyTimeline.	 * 	 * The TweensyTimelineZero Class has events for:	 * <ul>	 * <li>onComplete - When all tweens in the TweensyTimelineZero Class are done</li>	 * <li>onUpdate - When all animations in the TweensyTimelineZero Class have been updated.</li>	 * </ul>	 * These events allow for predefined params to be applied when they are executed.	 * Tweensy favours this method as it allows for predefining the event and the params it requires	 * without all the fussiness of Event listeners.	 */	public class TweensyTimelineZero {
		/**		 * The time in seconds the TweensyTimeline has been running for		 */		public var time : Number = 0;		/**		 * The Object instance whose properties are being tweened		 */		public var instance : *;		/**		 * This is the Object used as a reference to determining wether to stop this instance in tween conflict		 */		public var key : Object;		/**		 * Defines the ease equation you would like to use. By default this is Quintic.easeOut or the defaultTween.		 */		public var ease : Function;		public var easeParams : Array;		/**		 * The number of properties being tweened in the timeline.		 */		public var properties : int = 0;		/**		 * The to properties for the tween - where the tween will end.		 */		public var to : Object;		/**		 * The from properties for the tween - where the tween started.		 */		public var from : Object;		/**		 * Sets and gets the current tween duration for the TweensyTimeline in seconds.		 */		public var duration : Number = 0;		/**		 * Defines the delay in seconds at the start of the animation.		 */		public var delayStart : Number = 0;		/**		 * Defines the delay in seconds at the end of the animation.		 */		public var delayEnd : Number = 0;		/**		 * Executed on each update to the TweensyTimeline animation.		 * 		 * @see com.flashdynamix.motion.TweensyTimelineZero#onUpdateParams		 */		public var onUpdate : Function;		/**		 * Parameters applied to the onUpdate Function.		 * 		 * @see com.flashdynamix.motion.TweensyTimelineZero#onUpdate		 */		public var onUpdateParams : Array;		/**		 * Executed when the TweensyTimelineZero animation is complete.		 * 		 * @see com.flashdynamix.motion.TweensyTimelineZero#onCompleteParams		 */		public var onComplete : Function;		/**		 * Parameters applied to the onComplete Function.		 * 		 * @see com.flashdynamix.motion.TweensyTimelineZero#onComplete		 */		public var onCompleteParams : Array;		/**		 * The Object updated when the tween updates, this is used for BitmapFilter, ColorTransform, Matrix and SoundTransform tweens.<BR>		 * In the case of BitmapFilter, BitmapFilter and Matrix this update Object would be a DisplayObject.<BR>		 * For SoundTransforms the update Object would be a SoundChannel.		 */		public var update : *;
